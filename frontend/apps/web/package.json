    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.4",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-headless-table": "^0.18.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "uid": "^2.0.2",
    "vite": "^5.4.14",
    "vitest": "^2.1.8"
  }
